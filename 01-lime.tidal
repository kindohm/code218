bps (140/120)

do
    let spat = "[1.1 ~ ~ ~ ~ ~ 0.4 ~ ~ ~]"
    let fpat = "[0   ~ ~ ~ ~ ~ 127 ~ ~ ~]"
    let dpat = "[0   ~ ~ ~ ~ ~ 3   ~ ~ ~]"
    let mod p = spaceOut [1,1.1,0.9,0.95,1.7,1.25,0.8,1.65,1,0.7,0.823,1.5,1,2,1.8] $ p
